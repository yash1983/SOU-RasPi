{
  "api": {
    "base_url": "http://demotms.apphost.in/api/",
    "fetch_endpoint": "bookings/summary",
    "sync_endpoint": "bookings/update-used",
    "timeout": 10,
    "retry_attempts": 3,
    "retry_delay": 2
  },
  "services": {
    "fetch_interval": 10,
    "sync_interval": 5,
    "status_check_interval": 2,
    "fetch_enabled": true,
    "sync_enabled": true,
    "cleanup_enabled": true,
    "skip_dummy_sync": true,
    "add_dummy_tickets": false
  },
  "database": {
    "backup_enabled": true,
    "backup_interval": 3600,
    "max_backups": 7
  },
  "gate_mapping": {
    "A": "01",
    "B": "07",
    "C": "16"
  },
  "logging": {
    "level": "INFO",
    "file": "sou_system.log",
    "max_size": 10485760,
    "backup_count": 5
  }
}
